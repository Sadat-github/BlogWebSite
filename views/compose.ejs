<%- include('partials/header'); -%>
<h1>Compose</h1>
<form method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" class="form-control" id="exampleFormControlInput1">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postBody" id="exampleFormControlTextarea1" rows="5" cols="30"></textarea>
